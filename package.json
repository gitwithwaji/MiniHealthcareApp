{
  "name": "minihealthcareapp",
  "version": "1.0.0",
  "description": "This repository is a test demo for automating test cases on a Healthcare application. The repo is not a final version of any testing frameworks.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "dev:server": "cd backend && nodemon server.js",
    "test:api": "cd healthcare-qa-automation && mvn test"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.1.11"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
